    "lint:fix": "eslint --fix \"**/*.{js,ts}\"",
    "format": "prettier --check \"**/*.{ts,js}\"",
    "format:fix": "prettier --write \"**/*.{ts,js}\""
  },
  "devDependencies": {
    "@types/mocha": "^8.2.2",
