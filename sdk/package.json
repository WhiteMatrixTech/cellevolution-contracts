    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.3",
    "lint": "^0.7.0",
    "mocha": "^8.4.0",
    "nyc": "^15.1.0",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.2"
  },
  "dependencies": {
    "ethers": "^5.2.0"
  },
  "publishConfig": {
